{
	"name": "dataflowStage",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Pettsv",
						"type": "DatasetReference"
					},
					"name": "sourceStage"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "TransposeColumn"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {License Issue Date     License Number     Animal's Name     Species     Primary Breed     Secondary Breed     ZIP Code} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceStage",
				"sourceStage derive() ~> TransposeColumn"
			]
		}
	}
}